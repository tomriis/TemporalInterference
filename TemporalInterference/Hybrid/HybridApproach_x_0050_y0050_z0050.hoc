// Extracellular potential
objref a
objref fil
proc insert_electracellular() { strdef filename
     filename = $s1
     fil = new File()
     fil.ropen(filename)
     a = new Vector()
     a.scanf(fil)
     
     forall {
     	    i = int(n3d()/2)
	    e_extracellular(0.5) = a.x[i]
	    }
}

     
