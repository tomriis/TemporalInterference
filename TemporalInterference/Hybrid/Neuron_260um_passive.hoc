// DEFINE THE NEURON ELECTRICAL CHARACTERISTICS //

// -------------------------
// passive & active membrane 
// -------------------------

Ra0 = 100         // in Ohm.cm
cm0 = 1	      // in µF/cm2
gl0 = 1e-4      // in Ohm/cm2

v_init    = -65
// celsius   = 37

forall {
  Ra = Ra0
  cm = cm0

  insert hh
  gl_hh = gl0
  gnabar_hh = 0.12
  gkbar_hh = 0.036
  el_hh = -54.3

  insert extracellular
}


